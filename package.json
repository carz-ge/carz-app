{
  "name": "carbook-new",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "tunnel": "expo start --tunnel",
    "android": "expo start --android -c",
    "ios": "expo start --ios -c",
    "web": "expo start --web -c",
    "android:native": "expo run:android",
    "ios:native": "expo run:ios",
    "android:device": "expo run:android --device",
    "ios:device": "expo run:ios --device",
    "prebuild": "expo prebuild --clean",
    "test": "jest --watchAll",
    "lint": "eslint {lib,app,graphql,components}/**/*.{js,ts,tsx,jsx}",
    "lint:fix": "eslint {lib,app,graphql,components}/**/*.{js,ts,tsx,jsx} --fix",
    "prettier": "prettier {lib,app,graphql,components}/**/*.{js,ts,tsx,jsx} --debug-check ",
    "prettier:fix": "prettier {lib,app,graphql,components}/**/*.{js,ts,tsx,jsx} --write",
    "gen": "npm run gqlg && graphql-codegen --config codegen.ts && npm run lint:fix",
    "gqlg": "gqlg --schemaFilePath ./graphql/schema.graphql --destDirPath graphql/operations-gqls/generated --depthLimit 5",
    "doctor": "npx expo-doctor",
    "eas:update": "eas update --auto",
    "eas:build": "eas build -p android"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "^4.4.6",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@tanstack/react-query": "^4.29.11",
    "expo": "~48.0.18",
    "expo-barcode-scanner": "~12.3.2",
    "expo-clipboard": "~4.1.2",
    "expo-device": "~5.2.1",
    "expo-font": "~11.1.1",
    "expo-linking": "~4.0.1",
    "expo-notifications": "~0.18.1",
    "expo-router": "1.6.0",
    "expo-secure-store": "^12.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-updates": "~0.16.4",
    "expo-web-browser": "~12.1.1",
    "graphql": "^16.6.0",
    "jwt-decode": "^3.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.1",
    "react-native": "0.71.8",
    "react-native-calendars": "^1.1298.0",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-maps": "1.3.2",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.12",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "~18.0.38",
    "@types/react-native": "^0.72.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "gql-generator": "^1.0.19",
    "jest": "^29.2.2",
    "jest-expo": "~48.0.2",
    "null-loader": "^4.0.1",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "private": true
}
