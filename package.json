    "expo-secure-store": "^12.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-standard-web-crypto": "^1.5.0",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-updates": "~0.16.4",
    "expo-web-browser": "~12.1.1",
    "geolib": "^3.3.4",
    "graphql": "^16.7.1",
    "jwt-decode": "^3.1.2",
    "metro": "^0.76.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-native": "0.71.8",
    "react-native-app-link": "^1.0.1",
