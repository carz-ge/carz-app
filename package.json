{
  "name": "carz",
  "version": "1.0.0",
  "scripts": {
    "tsc-build": "tsc --build --verbose tsconfig-build.json",
    "start": "expo start --dev-client",
    "tunnel": "expo start --tunnel",
    "android": "expo start --android -c",
    "ios": "expo start --ios -c",
    "web": "expo start --web -c",
    "android:native": "expo run:android",
    "ios:native": "expo run:ios",
    "android:device": "expo run:android --device",
    "ios:device": "expo run:ios --device",
    "prebuild": "expo prebuild --clean",
    "test": "jest --watchAll",
    "lint": "eslint src App.tsx && npm run tsc-build",
    "lint:fix": "eslint src App.tsx --fix && npm run tsc-build",
    "prettier": "prettier src App.tsx --debug-check ",
    "prettier:fix": "prettier src App.tsx --write",
    "gen": "npm run gqlg && graphql-codegen --config codegen.ts && npm run lint:fix",
    "gqlg": "gqlg --schemaFilePath ./graphql/schema.graphql --destDirPath graphql/operations-gqls/generated --depthLimit 5",
    "doctor": "npx expo-doctor",
    "eas:update": "eas update --auto",
    "eas:build": "eas build -p android",
    "prod": "npx expo start --no-dev --minify",
    "preview": " eas build --profile preview --platform android",
    "prepare": "husky install"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "^4.4.7",
