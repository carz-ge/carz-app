    "typescript": "^4.9.5"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
